{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "baseUrl": ".",
    "declaration": true
  },
  "exclude": [
    "node_modules",
    "cache"
  ]
}